html, body
    width 100%
    height 100%

body
    -webkit-app-region drag
    background #191919
    color #111
    cursor default
    user-select none

input
    -webkit-app-region no-drag

body, input, textarea, button
    font-family "Rounded Mplus 1c"

input, textarea, button
    background none
    border none
    margin 0
    outline none
    padding 0

button
    cursor pointer

h1, h2, h3, h4, h5, h6
    font-size 100%
    font-weight normal